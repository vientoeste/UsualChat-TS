{
    "compilerOptions": {
      "target": "ES2019",
      "module": "commonjs",
      "strict": true,
      "outDir": "dist",
      "sourceMap": true,
      "removeComments": true,
      "noImplicitAny": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "resolveJsonModule": true,
    },
    "exclude": [
      "node_modules"
    ],
  }